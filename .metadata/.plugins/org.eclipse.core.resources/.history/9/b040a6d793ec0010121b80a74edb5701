
public class DiaSemana {

	public static void main(String[] args) {
		
		int numero = 10;
		String dia;
		switch (numero) {
		case 1: 
			dia = "domingo";
			break;
		case 2:
			dia = "segunda";
			break;
		default:
			throw new IllegalArgumentException("Unexpected value: " + numero);
		}
		
		System.out.println(dia);

	}

}
